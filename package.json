{
  "name": "rusty-object-notation",
  "version": "0.0.2",
  "description": "Rusty Object Notation for Javascript",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "scripts": {
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "build-type-declarations": "npx tsc --emitDeclarationOnly --declaration ./src/index.ts --outDir ./dist/declarations",
    "build": "node scripts/build.mjs && npm run build-type-declarations"
  },
  "types": "dist/declarations/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/whiteand/rusty-object-notation.git"
  },
  "keywords": [
    "ron",
    "rusty",
    "rust",
    "object",
    "notation"
  ],
  "author": "whiteand",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/whiteand/rusty-object-notation/issues"
  },
  "homepage": "https://github.com/whiteand/rusty-object-notation#readme",
  "devDependencies": {
    "esbuild": "^0.18.2",
    "typescript": "^5.1.3",
    "vitest": "^0.32.0"
  }
}
